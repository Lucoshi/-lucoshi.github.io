	<script type="text/javascript">
		$(document).ready(function () {
		
		var currentFrame;
		var centerStageSpeed = 1600;
		var centerStageInterval = 6000;
		var paginationInterval;
		
		function onBefore(currSlideElement, nextSlideElement, options) {
			removeCurrentPagination();
			paginationInterval = setInterval(animateCurrentPagination, centerStageInterval/4.5);
			animateCurrentPagination();			
			insertBanner("#"+nextSlideElement.id, nextSlideElement.rel, nextSlideElement.name);
		}
	
		function insertBanner(elem, imgUrl, elemBG) {
				$("#mainContentArea").stop().animate({'background-color': String(elemBG)}, centerStageSpeed-300); //average estimation for fade in of BG
				
				//if element exist
				if ($(elem +" a img").length > 0){
					//element exist
				} else {
					var imgLoader = new Image();
					$(imgLoader).attr('src', imgUrl);
					$(imgLoader).load(function () {	
						$(elem).html('<img border="0" src="'+imgUrl+'" />');
					});
				}
		}
		
		function animateCurrentPagination() {
			currentFrame = (currentFrame >= 5) ? currentFrame=1 : currentFrame+1;			
			var showFrame = "frame" +currentFrame;
			
			$(".activeSlide").removeClass().addClass("activeSlide").addClass(showFrame);
			
		}
		
		function removeCurrentPagination() {
			clearInterval(paginationInterval);
			currentFrame = 0;
			$(".activeSlide").removeClass()
		}
		
		
		function pageClick(zeroBasedSlideIndex, slideElement)   {
			elemBG = slideElement.name;
			$("#mainContentArea").css({'background-color': String(elemBG)});
		}
		
		
		$('#viewport').cycle({
				fx: 'fade', 
				speed:  centerStageSpeed,
				timeout: 6000,
				pause: 1,
				width: 977,
				height: 431,
				pager: '#viewport_pag',
				before: onBefore,
				onPagerEvent: pageClick 
			});
		
		
		$("#smfeatures").carouFredSel({
			  width: 980,
			  height: 174,
			  auto: false,
			  items: {
				  visible: 4,
				  height: 174,
				  width: 249
			  },
			  scroll: {
				  items: 4
			  },
			  prev : {
			  	button		: "#featPrevious",
				duration	: 600
			  },
			  next : {
			  		button: "#featNext",
					duration: 600,
					easing: "easeInOutQuad"
				},
			  pagination: {
			  	container: "#smfeatures_pag",
				duration: 800,
				easing: "swing"
			  }
		  });
		
				
		$("#rlist").carouFredSel({
			width: 660,
			height: 16,
			auto: true,
			direction: "up",
			items: {
				visible: 1,
				width: 660,
				height: 16,
				top: 6
			},
			scroll: {
				items: 1,
				duration: 1200,
				pauseOnHover: true
			}
		});
		
		$("#wlist").carouFredSel({
			width: 660,
			height: 16,
			auto: true,
			direction: "up",
			items: {
				visible: 1,
				width: 660,
				height: 16,
				top: 6
			},
			scroll: {
				items: 1,
				duration: 1600,
				pauseOnHover: true
			}
		});				  
		});
		
		
		tray = {
		isOpen: true,
	
		open : function(){
				$(".traytitle").fadeIn();
				$(".systemlist").fadeIn();
				$("#hwarea").animate({top: -12});
				$(".viewbtn").css("background-image", "url('images/home/features/hardware_close.png')");
				$(".viewbtn").animate({top: -2});
				$("#topSystemsArea").animate({height: 278});
				tray.isOpen = true;
				Omniture.trackLink(6, "Cookie Tray", 32, "Cookie Tray");
			},
		close : function(dontAnimate){		
				if(dontAnimate){
					$(".traytitle").hide();
					$(".systemlist").hide();
					$("#hwarea").css({top: 0});
					$(".viewbtn").css('top', '0');
					$("#topSystemsArea").css('height', '0');
				} else { 		
					$(".traytitle").fadeOut();
					$(".systemlist").fadeOut();
					$("#hwarea").animate({top: 0});					
					$(".viewbtn").animate({top: 0});
					$("#topSystemsArea").animate({height: 0});
					Omniture.trackLink(6, "Cookie Tray", 33, "Cookie Tray");
				}
				$(".viewbtn").css("backgroundImage", "url('images/home/features/hardware_open.png')");
				tray.isOpen = false;
			}
		};
		
	$(".viewbtn").live('click', function() {
		if(tray.isOpen) {
			tray.close();		
		} else {
			tray.open();
		}
		
	});
	
	if ($.browser.msie && $.browser.version=="6.0") {
		tray.open(); //leave tray open
		$(".viewbtn").hide(); //disable viewbutton
	} else {
	  tray.close(true); //close without opening
	}
	
	//Omniture
	Omniture.config('home');
	Omniture.pageTrack('home', true); //pagename, isHome, isVanity
	

	//Initialize tray to open on page load and close a few seconds later
	tray.open();
	var trayClose = setTimeout("tray.close()", 3000); 


	</script>

